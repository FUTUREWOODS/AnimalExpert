import urllib2
import json

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)

    def onLoad(self):
        #put initialization code here
        pass

    def onUnload(self):
        #put clean-up code here
        pass

    def onInput_onStart(self):
        url = 'http://218.251.224.154:5000/' + urllib2.quote(self.getParameter("name"))
        response = urllib2.urlopen(url)
        rjson = json.load(response)

        self.logger.info("[ConsoleMessage] %s" % rjson)
        if len(rjson) == 0:
            self.onError()
        data = rjson[u'0']
        name = data[u'name'].encode('utf-8')
        description = data[u'description'].encode('utf-8')
        img_url = data[u'img_url'].encode('utf-8')
        img_url = img_url.replace("http","https",1)
        self.description(description)
        self.animalName(name)
        self.imageURL(img_url)

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box]]>
